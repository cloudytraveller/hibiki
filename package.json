{
  "type": "module",
  "private": "true",
  "name": "@hibikiapp/hibiki",
  "version": "5.0.0-alpha",
  "description": "The best all-in-one Discord bot.",
  "license": "zlib",
  "author": "Espi <espi@riseup.net> (https://espi.me)",
  "contributors": [
    "Flleeppyy <flleeppyybusiness@gmail.com> (https://fleepy.tv/)"
  ],
  "homepage": "https://github.com/sysdotini/hibiki#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysdotini/hibiki.git"
  },
  "bugs": {
    "url": "https://github.com/sysdotini/hibiki/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:static": "cross-env copyfiles './src/web/**/**/*.{css,liquid,jpg,js,png}' './dist/'",
    "dev": "nodemon ./src/index.ts",
    "lint": "cross-env eslint --fix .",
    "format": "cross-env prettier --write './**/**/*.{js,json,sass,scss,liquid,md,ts,yml}'",
    "prebuild": "cross-env rimraf ./dist/",
    "postbuild": "npm run build:static",
    "start": "npm run build && cross-env NODE_ENV=production node --experimental-modules ./dist/src/index.js",
    "test": "cross-env tsc --noEmit && eslint . && prettier --c ."
  },
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@fastify/accepts": "^3.0.0",
    "@fastify/cookie": "^6.0.0",
    "@fastify/cors": "^7.0.0",
    "@fastify/csrf": "^4.0.1",
    "@fastify/helmet": "^8.0.1",
    "@fastify/oauth2": "^5.0.0",
    "@fastify/session": "^8.2.0",
    "@fastify/static": "^5.0.2",
    "discord-api-types": "^0.32.1",
    "discord.js": "^13.7.0",
    "fastify": "^3.29.0",
    "fastify-secure-session": "^3.2.0",
    "liquidjs": "^9.37.0",
    "pino": "^7.11.0",
    "pino-pretty": "^7.6.1",
    "point-of-view": "^5.3.0"
  },
  "devDependencies": {
    "@sysdotini/eslint-config": "^3.1.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "4.7.0-beta"
  }
}
