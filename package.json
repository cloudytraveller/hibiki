{
  "type": "module",
  "private": "true",
  "name": "@hibikiapp/hibiki",
  "version": "5.0.0-alpha",
  "description": "The best all-in-one Discord bot.",
  "license": "zlib",
  "author": "Espi Marisa <espi@riseup.net> (https://espi.me)",
  "contributors": [
    "Flleeppyy <flleeppyybusiness@gmail.com> (https://fleepy.tv/)"
  ],
  "homepage": "https://github.com/sysdotini/hibiki#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysdotini/hibiki.git"
  },
  "bugs": {
    "url": "https://github.com/sysdotini/hibiki/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run prisma:dev && nodemon ./src/index.ts",
    "lint": "cross-env eslint --fix .",
    "format": "cross-env prettier --write './**/**/*.{js,json,sass,scss,liquid,md,ts,yml}'",
    "prebuild": "cross-env rimraf ./dist/",
    "prisma:dev": "cross-env prisma db push",
    "prisma:dev:reset": "prisma migrate reset",
    "prisma:prod": "cross-env prisma migrate deploy",
    "start": "npm run build && npm run prisma:prod && cross-env NODE_ENV=production node ./dist/src/index.js",
    "test": "cross-env tsc --noEmit && eslint . && prettier --c ."
  },
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@prisma/client": "^3.14.0",
    "discord-api-types": "^0.33.2",
    "discord.js": "^13.7.0",
    "pg": "^8.7.3",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "prisma": "^3.14.0"
  },
  "devDependencies": {
    "@sysdotini/eslint-config": "^3.1.1",
    "@types/node": "^17.0.39",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3"
  }
}
