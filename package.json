{
  "type": "module",
  "private": "true",
  "name": "@hibikiapp/hibiki",
  "version": "5.0.0-alpha",
  "description": "The best all-in-one Discord bot.",
  "license": "zlib",
  "author": "Espi Marisa <espi@riseup.net> (https://espi.me)",
  "contributors": [
    "Flleeppyy <flleeppyybusiness@gmail.com> (https://fleepy.tv/)"
  ],
  "homepage": "https://github.com/sysdotini/hibiki#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysdotini/hibiki.git"
  },
  "bugs": {
    "url": "https://github.com/sysdotini/hibiki/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run prisma:dev && nodemon ./src/index.ts",
    "lint": "cross-env eslint --fix .",
    "format": "cross-env prettier --write './**/**/*.{js,json,sass,scss,liquid,md,ts,yml}'",
    "prebuild": "cross-env rimraf ./dist/",
    "prisma:dev": "cross-env prisma db push",
    "prisma:dev:reset": "prisma migrate reset",
    "prisma:prod": "cross-env prisma migrate deploy",
    "start": "npm run build && npm run prisma:prod && cross-env NODE_ENV=production node --no-warnings ./dist/src/index.js",
    "test": "cross-env tsc --noEmit && eslint . && prettier --c ."
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.1.0",
    "@discordjs/voice": "^0.11.0",
    "@prisma/client": "^4.2.1",
    "discord-api-types": "^0.37.5",
    "discord.js": "^14.3.0",
    "pg": "^8.8.0",
    "pino": "^8.4.2",
    "pino-pretty": "^9.1.0",
    "prisma": "^4.2.1",
    "sodium-native": "^3.4.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@sysdotini/eslint-config": "^3.1.1",
    "@types/node": "^18.7.13",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  }
}
